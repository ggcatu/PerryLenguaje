//Preuba whiles

main{
	create{
		int inte;
		float floatp;
		bool boole;
		array int [5] harray;
		tuple (bool,float) twoforone;
	}
	execute{
		while(boole and ((inte == harray[3]) or not(twoforone[1] != floatp))){
			inte = inte + floatp;
			inte = inte - floatp;
			inte = inte / floatp;
			inte = inte * floatp;
			inte = inte mod floatp;
			inte = inte ** floatp;

			boole = inte > floatp;
			boole = inte < floatp;
			boole = inte >= floatp;
			boole = inte <= floatp;
			break;
		};
	};
}
